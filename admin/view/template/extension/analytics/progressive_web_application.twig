<div id="content">
  <div class="page-header">
    <div class="container-fluid">
      <div class="pull-right">
        <button type="submit" form="form-progressive-web-application-general" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary"><i class="fa fa-save"></i>&nbsp;&nbsp;&nbsp;{{ button_save }}</button>
        <a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i>&nbsp;&nbsp;&nbsp;{{ button_cancel }}</a></div>
        <h1>{{ heading_title }}</h1>
        <ul class="breadcrumb">
          {% for key, breadcrumb in breadcrumbs %}  
          <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
          {% endfor %} 
        </ul>
      </div>
    </div>
    <div class="container-fluid">
      {% if error_warning %} 
      <div class="alert alert-danger"><i class="fa fa-exclamation-circle"></i>{{ error_warning }} 
        <button type="button" class="close" data-dismiss="alert">&times;</button>
      </div>
      {% endif %} 
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title"><i class="fa fa-pencil"></i>&nbsp;&nbsp;{{ text_edit }}</h3>
          <span class="pull-right">Extension by:&nbsp;&nbsp;<a href="mailto:todor.donev@gmail.com">Todor Donev</a></span>
        </div>
        <div class="panel-body">

