            <div class="form-group">
              <div class="col-sm-1">&nbsp;</div>
              <div class="col-sm-10 alert alert-info">{{ text_about }}</div>
            </div>
            <div class="form-group">
              <label class="col-sm-4 control-label" for="input-status"><span data-toggle="tooltip" title="{{ help_status }}">{{ entry_status }}</span></label>
              <div class="col-sm-4 col-sm-offset-1">
                <select name="analytics_progressive_web_application_status[status]" id="input-status" class="form-control">
                  {% if status %} 
                  <option value="1" selected="selected">{{ text_activated }}</option>
                  <option value="0">{{ text_deactivated }}</option>
                  {% else %}   
                  <option value="1">{{ text_activated }}</option>
                  <option value="0" selected="selected">{{ text_deactivated }}</option>
                  {% endif %}  
                </select>
              </div>
            </div>

            <hr>

            <div class="col-sm-12">
             <!-- START MENU -->
             <ul class="nav nav-tabs">
              <li class="active"><a data-toggle="tab" href="#tab-general"><b><i class="fa fa-cog"></i>&nbsp;&nbsp;{{ tab_general }}</b></a></li>
              <li><a data-toggle="tab" href="#tab-support"><b><i class="fa fa-life-ring"></i>&nbsp;&nbsp;{{ tab_support }}</b></a></li>
            </ul>
            <!-- END MENU -->

