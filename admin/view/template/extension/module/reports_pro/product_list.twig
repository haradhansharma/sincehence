  <div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title"><i class="fa fa-list"></i> {{ text_list }}</h3>
        </div>
        <div class="panel-body">
          <form action="{{ generate_seo_url }}" method="post" enctype="multipart/form-data" id="form-product">
            <div class="table-responsive">
              <table class="table table-bordered table-hover" id="product-table">
                <thead>
                  <tr>
                    <td style="width: 1px;" class="text-center"><input type="checkbox" onclick="$('input[name*=\'selected\']').prop('checked', this.checked);" /></td>

                    <td class="text-center" id="th-image">{{ column_image }}</td>
                    <td class="text-left" id="th-name">{% if sort == 'pd.name' %} <a href="{{ sort_name }}" class="{{ order|lower }}">{{ column_name }}</a> {% else %} <a href="{{ sort_name }}">{{ column_name }}</a> {% endif %}</td>

                    <td class="text-left" id="th-model">{% if sort == 'p.model' %} <a href="{{ sort_model }}" class="{{ order|lower }}">{{ column_model }}</a> {% else %} <a href="{{ sort_model }}">{{ column_model }}</a> {% endif %}</td>

                    <td class="text-right" id="th-price">{% if sort == 'p.price' %} <a href="{{ sort_price }}" class="{{ order|lower }}">{{ column_price }}</a> {% else %} <a href="{{ sort_price }}">{{ column_price }}</a> {% endif %}</td>

                    <td class="text-right" id="th-quantity">{% if sort == 'p.quantity' %} <a href="{{ sort_quantity }}" class="{{ order|lower }}">{{ column_quantity }}</a> {% else %} <a href="{{ sort_quantity }}">{{ column_quantity }}</a> {% endif %}</td>

                    <td class="text-left" id="th-status">{% if sort == 'p.status' %} <a href="{{ sort_status }}" class="{{ order|lower }}">{{ column_status }}</a> {% else %} <a href="{{ sort_status }}">{{ column_status }}</a> {% endif %}</td>

                    <td class="text-left" id="th-meta-title" style="display:none">{% if sort == 'pd.meta_title' %} <a href="{{ sort_meta_title }}" class="{{ order|lower }}">{{ column_meta_title }}</a> {% else %} <a href="{{ sort_meta_title }}">{{ column_meta_title }}</a> {% endif %}</td>

                    <td class="text-left" id="th-meta-keyword" style="display:none">{% if sort == 'pd.meta_keyword' %} <a href="{{ sort_meta_keyword }}" class="{{ order|lower }}">{{ column_meta_keyword }}</a> {% else %} <a href="{{ sort_meta_keyword }}">{{ column_meta_keyword }}</a> {% endif %}</td>

                    <td class="text-left" id="th-tags" style="display:none">{% if sort == 'pd.tag' %} <a href="{{ sort_tag }}" class="{{ order|lower }}">{{ column_tags }}</a> {% else %} <a href="{{ sort_tag }}">{{ column_tags }}</a> {% endif %}</td>

                    <td class="text-left" id="th-image-custom-title" style="display:none">{% if sort == 'p.image_custom_title' %} <a href="{{ sort_image_custom_title }}" class="{{ order|lower }}">{{ column_image_custom_title }}</a> {% else %} <a href="{{ sort_image_custom_title }}">{{ column_image_custom_title }}</a> {% endif %}</td>

                    <td class="text-left" id="th-image-custom-alt" style="display:none">{% if sort == 'p.image_custom_alt' %} <a href="{{ sort_image_custom_alt }}" class="{{ order|lower }}">{{ column_image_custom_alt }}</a> {% else %} <a href="{{ sort_image_custom_alt }}">{{ column_image_custom_alt }}</a> {% endif %}</td>
                    
                    <td class="text-center" id="th-seo-score">{{ column_seo_score }}</td>
                    <td class="text-center" id="th-readability-score">{{ column_readability_score }}</td>
                    
                    <td class="text-right">{{ column_action }}</td>
                  </tr>
                </thead>
                <tbody>
                  
                  {% if products %}
                    {% for product in products %}
                      <tr>
                        <input type="hidden" name="action" >
                        <td class="text-center">{% if product.product_id in selected %}
                        <input type="checkbox" name="selected[]" value="{{ product.product_id }}" checked="checked" />
                        {% else %}
                        <input type="checkbox" name="selected[]" value="{{ product.product_id }}" />
                        {% endif %}</td>

                        <td class="text-center" id="td-image">{% if product.image %} <img src="{{ product.image }}" alt="{{ product.name }}" class="img-thumbnail" /> {% else %} <span class="img-thumbnail list"><i class="fa fa-camera fa-2x"></i></span> {% endif %}</td>

                        <td class="text-left" id="td-name">{{ product.name }}</td>
                        <td class="text-left" id="td-model">{{ product.model }}</td>
                        <td class="text-right" id="td-price">{{ product.price }}</td>

                        <td class="text-right" id="td-quantity">{% if product.quantity <= 0 %} <span class="label label-warning">{{ product.quantity }}</span> {% elseif product.quantity <= 5 %} <span class="label label-danger">{{ product.quantity }}</span> {% else %} <span class="label label-success">{{ product.quantity }}</span> {% endif %}</td>

                        <td class="text-left" id="td-status">{{ product.status }}</td>

                        <td class="text-left" id="td-meta-title" style="display:none">{{ product.meta_title }}</td>

                        <td class="text-left" id="td-meta-keyword" style="display:none">{{ product.meta_keyword }}
                        </td>
                        <td class="text-left" id="td-tags" style="display:none">{{ product.tag }}</td>

                        <td class="text-left" id="td-image-custom-title" style="display:none">{{ product.image_custom_title }}</td>

                        <td class="text-left" id="td-image-custom-alt" style="display:none">{{ product.image_custom_alt }}</td>
                        
                        <td class="text-center" id="td-seo-score">

                          {% if product.seo_score >= 70 %}
                            <i class="fa fa-circle fa-lg text-success" title="{{ text_good }}"></i>
                          {% endif %}

                          {% if product.seo_score >= 50 and product.seo_score < 70 %}
                            <i class="fa fa-circle fa-lg text-warning" title="{{ text_acceptable }}"></i>
                          {% endif %}

                          {% if product.seo_score > 0 and product.seo_score < 50 %}
                             <i class="fa fa-circle fa-lg text-danger" title="{{ text_need_improvements }}" ></i>
                          {% endif %}

                          {% if product.seo_score <= 0 %}
                            <i class="fa fa-circle fa-lg text-default" title="{{ text_not_started }}"></i>
                          {% endif %}

                        </td>

                        <td class="text-center" id="td-readability-score">

                          {% if product.readability_score >= 60 %}
                            <i class="fa fa-circle fa-lg text-success" title="{{ text_good }}"></i>
                          {% endif %}

                          {% if product.readability_score >= 50 and product.readability_score < 60 %}
                            <i class="fa fa-circle fa-lg text-warning" title="{{ text_acceptable }}"></i>
                          {% endif %}

                          {% if product.readability_score > 0 and product.readability_score < 50 %}
                            <i class="fa fa-circle fa-lg text-danger" title="{{ text_need_improvements }}" ></i>
                          {% endif %}

                          {% if product.readability_score <= 0 %}
                            <i class="fa fa-circle fa-lg text-default" title="{{ text_not_started }}"></i>
                          {% endif %}

                        </td>

                        <td class="text-right"><a href="{{ product.edit }}" data-toggle="tooltip" title="{{ button_edit }}" class="btn btn-primary"><i class="fa fa-pencil"></i></a></td>
                    </tr>
                  {% endfor %}
                {% else %}
                  <tr>
                    <td class="text-center" colspan="10">{{ text_no_results }}</td>
                  </tr>
                {% endif %}
                </tbody>
              </table>
            </div>
          </form>
          <div class="row">
            <div class="col-sm-6 text-left">{{ pagination }}</div>
            <div class="col-sm-6 text-right">{{ results }}</div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>


<script type="text/javascript">
  $(document).ready(function(){
   $('input[name=\'screen-option\']').change(function(){

      $id = $(this).attr('data-id');
      
       if(this.checked)
       {
        $('#product-table').find('#th-'+$id).show();
        $('#product-table').find('td#td-'+$id).show();
       } else {
          $('#product-table').find('#th-'+$id).hide();
          $('#product-table').find('td#td-'+$id).hide();
       }
   });

});
</script>
  


<script type="text/javascript"><!--
$('#button-filter').on('click', function() {
  var url = '';
  var filter_name = $('input[name=\'filter_name\']').val();
  if (filter_name) {
    url += '&filter_name=' + encodeURIComponent(filter_name);
  }
  var filter_model = $('input[name=\'filter_model\']').val();
  if (filter_model) {
    url += '&filter_model=' + encodeURIComponent(filter_model);
  }

    var filter_meta_title_status = $('select[name=\'filter_meta_title_status\']').val();
  if (filter_meta_title_status !== '') {
    url += '&filter_meta_title_status=' + encodeURIComponent(filter_meta_title_status);
  }

   var filter_meta_description_status = $('select[name=\'filter_meta_description_status\']').val();
  if (filter_meta_description_status !== '') {
