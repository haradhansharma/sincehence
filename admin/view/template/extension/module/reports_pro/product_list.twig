    <div class="panel panel-default">
      <div class="panel-heading">
          <h3 class="panel-title">{{ text_screen_options }}</h3>
      </div>
      <div class="panel-body">
        <div class="row">
          <div class="col-lg-2 col-md-3">
            <input type="checkbox" data-id="image" name="screen-option" checked=""> {{ column_image }}
          </div>
          <div class="col-lg-2 col-md-3">
            <input type="checkbox" data-id="name" name="screen-option" checked=""> {{ column_name }}
          </div>
          <div class="col-lg-2 col-md-3">
            <input type="checkbox" data-id="model" name="screen-option" checked=""> {{ column_model }}
          </div>
          <div class="col-lg-2 col-md-3">
            <input type="checkbox" data-id="price" name="screen-option" checked=""> {{ column_price }}
          </div>
          <div class="col-lg-2 col-md-3">
            <input type="checkbox" data-id="quantity" name="screen-option" checked=""> {{ column_quantity }}
          </div>
